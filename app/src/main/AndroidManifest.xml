<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shoudukejiguan.www">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.FLASHLIGHT" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.MoreNewsActivity" />
        <activity
            android:name=".activity.CaptureActivity"
            android:screenOrientation="portrait"
            android:stateNotNeeded="true"
            android:windowSoftInputMode="stateAlwaysHidden" />
        <activity android:name=".activity.WebViewActivity" />
        <activity android:name=".activity.CinemaActivity" />
        <activity android:name=".activity.EducationActivity" />
        <activity android:name=".activity.ExhibitionActivity" />
        <activity android:name=".activity.SetActivity" />
        <activity android:name=".activity.UserInfoActivity" />
        <activity android:name=".activity.PayActivity"></activity>
        <activity android:name=".activity.ServiceGuideActivity"></activity>
        <activity android:name=".activity.UserLevelActivity"></activity>
        <activity android:name=".activity.MyOrderActivity" />
        <activity android:name=".activity.InteractionFeedbackActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.WriteSurveyActivity"></activity>
    </application>

</manifest>